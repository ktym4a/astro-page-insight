{
	"name": "astro-page-insight",
	"version": "0.8.1",
	"description": "Shows everything to improve from Lighthouse results directly on the page.",
	"author": {
		"email": "shoma@ktym4a.me",
		"name": "ktym4a",
		"url": "https://ktym4a.me"
	},
	"license": "MIT",
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"lighthouse",
		"page-insight",
		"a11y",
		"optimization",
		"devtools"
	],
	"homepage": "https://astro-page-insight.pages.dev/",
	"repository": "https://github.com/ktym4a/astro-page-insight.git",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"types": "./dist/integration.d.ts",
	"exports": {
		".": {
			"types": "./dist/integration.d.ts",
			"import": "./dist/integration.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"test:unit": "vitest run --coverage",
		"test:unit:dev": "vitest --coverage.enabled=true",
		"test:e2e": "playwright test",
		"test:e2e:install": "playwright install --with-deps && npx puppeteer browsers install chrome"
	},
	"type": "module",
	"peerDependencies": {
		"astro": "^4.0.0"
	},
	"dependencies": {
		"astro-integration-kit": "^0.13.2",
		"lighthouse": "^12.0.0",
		"puppeteer": "^22.7.1"
	},
	"devDependencies": {
		"@playwright/test": "^1.43.1",
		"@types/node": "^20.12.8",
		"@vitest/coverage-v8": "^1.6.0",
		"happy-dom": "^14.7.1",
		"tsup": "^8.0.2",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	}
}
