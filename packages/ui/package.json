{
	"name": "@page-insight/ui",
	"version": "0.0.0",
	"description": "UI components for Page Insight - displays Lighthouse results with a clean, accessible interface",
	"author": {
		"email": "shoma@ktym4a.me",
		"name": "ktym4a",
		"url": "https://ktym4a.me"
	},
	"license": "MIT",
	"keywords": [
		"page-insight",
		"lighthouse",
		"ui",
		"components",
		"devtools",
		"a11y",
		"performance"
	],
	"homepage": "https://github.com/ktym4a/astro-page-insight/tree/main/packages/ui",
	"repository": {
		"type": "git",
		"url": "https://github.com/ktym4a/astro-page-insight.git",
		"directory": "packages/ui"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./components/*": {
			"types": "./dist/components/*.d.ts",
			"import": "./dist/components/*.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"test:unit": "vitest run --coverage",
		"test:unit:dev": "vitest --coverage.enabled=true"
	},
	"devDependencies": {
		"@types/node": "^24.10.0",
		"@vitest/coverage-v8": "^4.0.8",
		"happy-dom": "^20.0.10",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vitest": "^4.0.8"
	}
}
